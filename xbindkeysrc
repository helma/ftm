  F1

"/home/ch/src/ftm/tag.rb 2"
  F2

"/home/ch/src/ftm/tag.rb 3"
  F3

"/home/ch/src/ftm/tag.rb DELETE"
  F12

"echo 'quit' > /tmp/mplayer-input"
  XF86AudioNext

"killall ftm-player.rb"
  XF86AudioStop

"echo 'pause' > /tmp/mplayer-input"
  XF86AudioPlay

"echo 'seek -10' > /tmp/mplayer-input"
  Control + XF86AudioPrev

"echo 'seek 10' > /tmp/mplayer-input"
  Control + XF86AudioNext

"/home/ch/src/ftm/info.rb|dmenu -b"
  XF86AudioPrev
